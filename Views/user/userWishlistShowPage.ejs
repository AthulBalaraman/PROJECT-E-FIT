    <!-- Wishlist  Start -->
    <div class="container-fluid pt-5">
        <div class="row px-xl-5">
            <div class="col-lg-12 table-responsive mb-5">
                <% if(products.length<=0) {%>
                    <h3>WISHLIST IS EMPTY</h3>
                    <% }else{ %>
                         

                <table class="table table-bordered text-center mb-0">
                    <thead class="bg-secondary text-dark">
                        <tr>
                            <th>Products</th>
                            <th>Image</th>
                            <th>Price</th>
                            <th>Add To Cart</th>
                            <th>Remove</th>
                        </tr>
                    </thead>
                    <tbody class="align-middle">
                        <% for(var i=0;i<products.length;i++) { %>
                        <tr>
                            <td class="align-middle"> <%= products[i].productDetails.productName%></td>
                            <td class="align-middle"><img src="/images/<%= products[i].productDetails.picture %>" alt="" style="width: 50px;"></td>
                            <td class="align-middle">â‚¹<%= products[i].productDetails.sellingPrice %></td>
                            <td class="align-middle"><button class="btn btn-sm btn-primary">ADD TO CART</button></td>
                            <td class="align-middle"><button onclick="removeWishListProduct('<%= products[i]._id %>','<%= products[i].productDetails._id %>')" class="btn btn-sm btn-primary"><i class="fa fa-times"></i></button></td>
                        </tr>
                        <% } %>
                       
                    </tbody>
                </table>
                <% } %> 
            </div>
            
        </div>
    </div>
    <!-- Wishlist End -->




    <!-- Back to Top -->
    <a href="#" class="btn btn-primary back-to-top"><i class="fa fa-angle-double-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="mail/jqBootstrapValidation.min.js"></script>
    <script src="mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>


    <script>
        function removeWishListProduct(listId,productId)
        {
           
            fetch(
                '/removeWishListProduct',{
                    method:'delete',
                    headers:{'Content-Type':'application/json'},
                body:JSON.stringify({
                    wishList:listId,
                    product:productId
                })
                
            }
            ).then((response)=>{
                    window.location.reload()
                })
        }
    </script>